{
  "extends": [
    "next",
    "prettier",
    "plugin:storybook/recommended"
  ],
  "rules": {
    "import/order": [
      "error",
      {
        "newlines-between": "always",
        "groups": [
          "builtin",
          "external",
          "internal",
          "unknown",
          "parent",
          "sibling",
          "index"
        ]
      }
    ]
  }
}
